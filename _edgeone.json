{
  "name": "myenglishai",
  "functions": {
    "dashscope-tts": {
      "runtime": "edge",
      "path": "/api/dashscope-tts",
      "file": "edge-functions/dashscope-tts/index.js",
      "env": {
        "DASHSCOPE_API_KEY": "{{DASHSCOPE_API_KEY}}"
      }
    }
  },
  "build": {
    "command": "npm run build",
    "outputDir": "dist"
  },
  "routes": [
    {
      "pattern": "/api/dashscope-tts",
      "function": "dashscope-tts"
    }
  ]
}
